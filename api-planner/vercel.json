{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "API_BASE_URL": "@api_base_url",
    "WEB_BASE_URL": "@web_base_url"
  }
}